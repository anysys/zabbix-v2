{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    },
  "variables": {
	"zabbixVnet":"zabbixVnet",
    "zabbixVnetPrefix": "192.168.0.0/16",
    "zabbixVnetSubnet1Name": "Subnet-1",
    "zabbixVnetSubnet1Prefix": "192.168.0.0/24"
    },
  "resources": [
    {
      "name": "zabbixVnet",
      "type": "Microsoft.Network/virtualNetworks",
      "location": "[resourceGroup().location]",
      "apiVersion": "2016-03-30",
      "dependsOn": [ ],
      "tags": {
        "displayName": "zabbixVnet"
      },
      "properties": {
        "addressSpace": {
          "addressPrefixes": [
            "[variables('zabbixVnetPrefix')]"
          ]
        },
        "subnets": [
          {
            "name": "[variables('zabbixVnetSubnet1Name')]",
            "properties": {
              "addressPrefix": "[variables('zabbixVnetSubnet1Prefix')]"
            }
          }
        ]
      }
    }
    ],
  "outputs": {
	"zabbixVnet":{
		"type":"string",
		"value":"[reference(variables('zabbixVnet'))]"
	},
	"zabbixSubnet":{
		"type":"string",
		"value":"[reference(variables('zabbixVnetSubnet1Name'))]"
	}
  }
}
